                      },
                      "href": {
                        "type": "string",
                        "description": "href",
                        "title": "",
                        "x-ms-visibility": "internal"
                      }
                    }
                  },
                  "description": "links"
                },
                "results": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "cage": {
                        "type": "string",
                        "description": "The Contractor and Government Entity (CAGE) Code or NATO Commercial and Government Entity (NCAGE) Code. This number is assigned to the registration if one does not already exist.",
                        "title": "CAGE/NCAGE code"
                      },
                      "hasKnownExclusion": {
                        "type": "boolean",
                        "description": "True or False value depending on whether the entity has had Exclusion created against it or not.",
                        "title": "Has Active Exclusion",
                        "enum": [
                          "",
                          "true",
                          "false"
                        ]
                      },
                      "legalBusinessName": {
