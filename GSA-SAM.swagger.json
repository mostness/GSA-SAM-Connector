    "/sam/v3/registrations": {
      "get": {
        "summary": "Search SAM Database",
        "description": "Find a list of business by name. Search by Legal Business Name, Doing-Business-As Name, DUNS, DUNS+4, CAGE Code, DoDAAC",
        "operationId": "Search",
        "parameters": [
          {
            "name": "qterms",
            "in": "query",
            "type": "string",
            "required": true,
            "description": "Query Parameter",
            "x-ms-summary": "See detail here: https://gsa.github.io/sam_api/sam/search.html"
          }
        ],
        "responses": {
          "200": {
            "description": "default",
            "schema": {
              "type": "object",
              "properties": {
                "links": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "rel": {
                        "type": "string",
                        "description": "rel",
                        "title": "",
